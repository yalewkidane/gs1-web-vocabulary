### place example 
POST http://localhost:3000/masterdata 
Content-Type: application/ld+json

{
  "@context": [
    {
      "cbvmda": "urn:epcglobal:cbv:mda",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "gs1": "http://gs1.org/voc/",
      "@vocab": "http://gs1.org/voc/",
      "gdst": "https://traceability-dialogue.org/vocab"
    },
    "https://ref.gs1.org/voc/data/gs1Voc.jsonld"
  ],
  "@type": "gs1:Place",
  "@id": "https://example.org/locations/importer-warehouse/3",
  "globalLocationNumber34": "7891234567895",
  "name": [{ "@language": "en-US", "@value": "Importer Warehouse" }],
  "address": {
    "@type": "gs1:PostalAddress",
    "streetAddress": [{ "@language": "en-US", "@value": "134 Estados Unidos" }],
    "addressLocality": [{ "@language": "en-US", "@value": "Sao Paulo" }],
    "addressRegion": [{ "@language": "en-US", "@value": "Sao Paulo" }],
    "countryCode": "BR"
  }
}

